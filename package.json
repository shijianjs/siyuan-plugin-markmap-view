{
  "name": "siyuan-plugin-markmap-view",
  "version": "1.0.0",
  "type": "module",
  "description": "思源 markmap 视图插件，以思维导图的形式展示当前笔记。",
  "repository": "https://github.com/shijianjs/siyuan-plugin-markmap-view",
  "homepage": "https://github.com/shijianjs/siyuan-plugin-markmap-view",
  "author": "shijianjs",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development VITE_SOURCEMAP=inline vite build --watch",
    "build": "cross-env NODE_ENV=production vite build",
    "make-link": "node --no-warnings ./scripts/make_dev_link.js",
    "make-link-win": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File ./scripts/elevate.ps1 -scriptPath ./scripts/make_dev_link.js",
    "update-version": "node --no-warnings ./scripts/update_version.js",
    "make-install": "vite build && node --no-warnings ./scripts/make_install.js"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@tsconfig/svelte": "^4.0.1",
    "@types/node": "^20.3.0",
    "cross-env": "^7.0.3",
    "fast-glob": "^3.2.12",
    "glob": "^10.0.0",
    "js-yaml": "^4.1.0",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.63.3",
    "siyuan": "1.1.2",
    "svelte": "^4.2.20",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vite": "^5.2.9",
    "vite-plugin-static-copy": "^1.0.2",
    "vite-plugin-zip-pack": "^1.0.5"
  },
  "dependencies": {
    "@siyuan-community/siyuan-sdk": "^0.3.14",
    "markmap-common": "^0.18.9",
    "markmap-lib": "^0.18.12",
    "markmap-render": "^0.18.12",
    "markmap-toolbar": "^0.18.12",
    "markmap-view": "^0.18.12"
  }
}